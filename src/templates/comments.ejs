<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Comments</title>
</head>
<body>
    <h1>All Comments</h1>
    <ul>
        <% comments.forEach(comment => { %>
        <li>
            <%= comment.name %> 
            <%= comment.content %>
            <br>
        </li>
        <% }); %>
    </ul>
</body>
</html>